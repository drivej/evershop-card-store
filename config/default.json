{
  "shop": {
    "language": "en",
    "currency": "USD"
  },
  "system": {
    "extensions": [
      {
        "name": "sample",
        "resolve": "extensions/sample",
        "enabled": true
      },
      {
        "name": "kinsta-simple",
        "resolve": "extensions/kinsta-simple",
        "enabled": true
      },
      {
        "name": "card-shipping",
        "resolve": "extensions/card-shipping",
        "enabled": true
      },
      {
        "name": "pokemon-hero",
        "resolve": "extensions/pokemon-hero",
        "enabled": true
      },
      {
        "name": "coupon",
        "resolve": "extensions/coupon",
        "enabled": true
      },
      {
        "name": "cardistry",
        "resolve": "extensions/cardistry",
        "enabled": true
      }
    ],
    "theme": "sample",
    "file_storage": "local"
  },
  "payment": {
    "stripe": {
      "secretKey": "${STRIPE_SECRET_KEY}",
      "publishableKey": "${STRIPE_PUBLISHABLE_KEY}",
      "isTestMode": true
    }
  },
  "pricing": {
    "rounding": "round",
    "precision": 2,
    "tax": {
      "rounding": "round",
      "precision": 2,
      "round_level": "unit",
      "price_including_tax": false
    }
  },
  "themeConfig": {
    "headTags": {
      "links": [
        {
          "rel": "preconnect",
          "href": "https://fonts.googleapis.com"
        },
        {
          "rel": "preconnect",
          "href": "https://fonts.gstatic.com",
          "crossorigin": ""
        },
        {
          "rel": "stylesheet",
          "href": "https://fonts.googleapis.com/css2?family=DM+Sans:ital,opsz,wght@0,9..40,100..1000;1,9..40,100..1000&display=swap"
        }
      ]
    }
  }
}
