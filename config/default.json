{
  "shop": {
    "language": "en",
    "currency": "USD"
  },
  "system": {
    "extensions": [
      {
        "name": "sample",
        "resolve": "extensions/sample",
        "enabled": true
      },
      {
        "name": "kinsta-simple",
        "resolve": "extensions/kinsta-simple",
        "enabled": true
      },
      {
        "name": "card-shipping",
        "resolve": "extensions/card-shipping",
        "enabled": true
      }
    ],
    "theme": "sample",
    "file_storage": "local"
  },
  "payment": {
    "stripe": {
      "secretKey": "${STRIPE_SECRET_KEY}",
      "publishableKey": "${STRIPE_PUBLISHABLE_KEY}",
      "isTestMode": true
    }
  },
  "pricing": {
    "rounding": "round",
    "precision": 2,
    "tax": {
      "rounding": "round",
      "precision": 2,
      "round_level": "unit",
      "price_including_tax": false
    }
  }
}
