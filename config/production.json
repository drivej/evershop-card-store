{
  "shop": {
    "language": "en",
    "currency": "USD"
  },
  "system": {
    "extensions": [
      {
        "name": "sample",
        "resolve": "extensions/sample",
        "enabled": false
      }
    ],
    "theme": "sample",
    "port": "${PORT}",
    "host": "0.0.0.0",
    "file_storage": "${FILE_STORAGE}",
    "s3": {
      "region": "${AWS_REGION}",
      "bucket": "${AWS_S3_BUCKET}",
      "accessKeyId": "${AWS_ACCESS_KEY_ID}",
      "secretAccessKey": "${AWS_SECRET_ACCESS_KEY}",
      "cdnUrl": "${AWS_CLOUDFRONT_URL}"
    },
    "kinsta": {
      "cdnUrl": "${KINSTA_CDN_URL}",
      "persistentStoragePath": "${KINSTA_STORAGE_PATH}",
      "publicUrl": "${KINSTA_PUBLIC_URL}"
    }
  },
  "payment": {
    "stripe": {
      "secretKey": "${STRIPE_SECRET_KEY}",
      "publishableKey": "${STRIPE_PUBLISHABLE_KEY}",
      "isTestMode": true
    }
  },
  "pricing": {
    "rounding": "round",
    "precision": 2,
    "tax": {
      "rounding": "round",
      "precision": 2,
      "round_level": "unit",
      "price_including_tax": false
    }
  },
  "database": {
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "database": "${DB_NAME}",
    "user": "${DB_USER}",
    "password": "${DB_PASSWORD}",
    "ssl": false,
    "pool": {
      "max": 5,
      "min": 0,
      "idle": 30000,
      "acquire": 30000
    }
  }
}
